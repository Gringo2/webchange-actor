{
    "name": "semantic-web-intelligent-monitor-swim",
    "version": "1.0.0",
    "description": "SWIM: Semantic Web Intelligent Monitor - AI-powered change detection (V1 & V2)",
    "main": "dist/main.js",
    "type": "module",
    "scripts": {
        "start": "npm run start:dev",
        "start:dev": "tsx src/main.ts",
        "start:prod": "node dist/main.js",
        "build": "tsc",
        "test": "node --loader ts-node/esm src/verify-v1.ts",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts"
    },
    "keywords": [
        "apify",
        "web-scraping",
        "monitoring",
        "ai",
        "semantic-diff"
    ],
    "author": "Antigravity",
    "license": "ISC",
    "dependencies": {
        "apify": "^3.1.10",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.3.1",
        "got-scraping": "^4.0.0",
        "openai": "^4.20.1",
        "puppeteer": "^24.35.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.35",
        "@types/node": "^20.19.28",
        "eslint": "^8.57.1",
        "prettier": "^3.7.4",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    }
}
